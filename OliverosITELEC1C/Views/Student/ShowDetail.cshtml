@model Student;

<h1>Student Details</h1>

<table class="table table-dark">
    <tr>
        <th>ID</th>
        <td>@Model.StudentId</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.StudentName</td>
    </tr>
    <tr>
        <th>Date Enrolled</th>
        <td>@Model.DateEnrolled</td>
    </tr>
    <tr>
        <th>Course</th>
        <td>@Model.Course</td>
    </tr>
    <tr>
        <th>University Email</th>
        <td>@Model.StudentEmail</td>
    </tr>
</table>